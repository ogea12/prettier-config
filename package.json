{
  "name": "@ogea12/prettier-config",
  "description": "Config' Prettier appliquÃ©e dans les projets de l'OGEA 12",
  "version": "1.0.7",
  "engines": {
    "node": ">=20.17.0"
  },
  "files": [
    "index.json"
  ],
  "exports": {
    ".": "./index.json"
  },
  "scripts": {
    "format": "prettier --write .",
    "prepare": "husky",
    "release": "npx release-it",
    "taze": "npx taze"
  },
  "homepage": "https://github.com/ogea12/prettier-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogea12/prettier-config.git"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "release-it": "^19.0.3",
    "taze": "^19.1.0"
  },
  "dependencies": {
    "prettier-plugin-edgejs": "^1.0.2"
  },
  "peerDependencies": {
    "prettier": "^3.6.2"
  },
  "author": "OGEA 12",
  "license": "MIT",
  "keywords": [
    "ogea12",
    "prettier"
  ],
  "prettier": "./index.json",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  }
}
